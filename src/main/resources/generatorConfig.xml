        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection connectionURL="jdbc:postgresql://192.168.1.12:10007/convertor_base"
                        driverClass="org.postgresql.Driver"
                        password="1234Qwer" userId="csnc_2019"></jdbcConnection>
        <javaTypeResolver>
